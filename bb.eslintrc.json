{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "react/no-array-index-key": "off",
        "react-hooks/exhaustive-deps": "off",
        "react/jsx-filename-extension": "off"
      }
    }
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  }
}
